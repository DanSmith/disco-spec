disco:ddi3_2identifier a rdf:Property, owl:DatatypeProperty ;
rdfs:label "DDI 3.1 identifier"@en, "DDI 3.2 Identifikation"@de ;
rdfs:domain rdf:Ressource ;
rdfs:range xsd:anyURI ;
