ddi:ARG_1908_PHC_v01_A_IPUMS-example a disco:Study , disco:LogicalDataset ;
	dc:title "National Population and Housing Census 1980"@en ;
	disco:isMeasureOf _:universe ;
	dcterms:spatial [
		# This is the DC-strictly compatible way to do it
		a dcterms:Location ;
		rdfs:label "Argentina, national"@en
	] ;
	dcterms:temporal [
		a dcterms:PeriodOfTime
		disco:startDate "1980-10-22"
		disco:endDate "1980-10-22"	
	] ;
	dcterms:subject [
		skos:definition "Technical Variables -- HOUSEHOLD"@en 
	] ;
	dcterms:subject [
		skos:definition "Group Quarters Variables -- HOUSEHOLD"@en 
	] ;
	disco:hasDatafile _:datafile ;
	disco:hasInstrument _:instrument ;
	disco:containsVariable ddi:AR80A401 , ddi:AR80A402 , ddi:AR80A404 , ddi:AR80A407 , ddi:AR80A411.

_:universe a disco:Universe ;
	skos:definition "All the population... carried out."@en .

_:instrument a disco:Instrument ;
	disco:usesQuestion _:question .

_:question a disco:Question ;
	disco:literalText "blabla"@en ;
	disco:hasResponseDomain _:sexCS .

_:datafile a disco:Datafile ;
	dc:identifier "ARG1900-P-H.dat" ;
	dc:description "Person records"@en ;
	disco:caseQuantity 2667714 ;
	dc:format "ascii" ;
	dc:provenance "Minnesota Population Center"@en ;
	owl:versionInfo "Version 1.0, IPUMS sample"@en ;
	dcterms:spatial [
		# This is the DC-strictly compatible way to do it
		a dcterms:Location ;
		rdfs:label "Argentina, national coverage"@en
	] ;
	dcterms:temporal "PeriodOfTime"@en ;
	dcterms:subject "To be defined"@en .

ddi:AR80A401 a disco:Variable ;
   dc:identifier "AR80A401" ;
   skos:prefLabel "Sex"@en , "Sexe"@fr ;
   dc:description "This variable indicates..."@en ;
   disco:isPopulatedBy _:instrument ;
   disco:usesDataElement _:sexDE ;
   disco:hasQuestion _:question .

_:sexDE a disco:DataElement ;
   disco:hasUniverse _:universe ;
   disco:hasRepresentation _:sexCS ;
   disco:hasConcept _:ipumsC1 ;
   skos:prefLabel "Sex"@en , "Sexe"@fr ;
   dc:description "Sex data element"@en .

_:dstat1 a disco:DescriptiveStatistic ;
   ddi:frequency 13314444 ;
   ddi:percentage 49.97 ;
   disco:hasStatisticsVariable ddi:AR80A401 ;
   disco:hasStatisticsCategory _:sexC1 ;
   disco:hasStatisticsDatafile _:datafile .

_:dstat2 a disco:DescriptiveStatistic ;
   ddi:frequency 1336270 ;
   disco:hasStatisticsVariable ddi:AR80A401 ;
   disco:hasStatisticsCategory _:sexC2 ;
   disco:hasStatisticsDatafile _:datafile .

_:sexCS a skos:ConceptScheme, disco:Representation ;
   skos:hasTopConcept _:sexC1 , _:sexC2 .

_:sexC1 a skos:Concept ;
   skos:notation "1" ;
   skos:prefLabel "Male"@en, "Homme"@fr ;
   skos:inScheme _:sexCS .

_:sexC2 a skos:Concept ;
   skos:notation "2" ;
   skos:prefLabel "Female"@en, "Femme"@fr ;
   skos:inScheme _:sexCS .

_:ipumsCS a skos:ConceptScheme ;
   skos:hasTopConcept _:ipumsC1 .

_:ipumsC1 a skos:Concept ;
   skos:prefLabel "Demographic Variables - PERSON"@en, "Variables démographiques - PERSONNE"@fr ;
   skos:inScheme _:ipumsCS .

